{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Suicide Rate in Countries in 2019",
  "width": "container",
  "height": 400,
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Bot1234-dotcom/3179_Homework_W9/refs/heads/main/js/WorldMapwithGraticules.topojson",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "transform": [
        {
          "lookup": "properties.NAME_CIAWF",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/Bot1234-dotcom/3179_Homework_W9/refs/heads/main/data/Suicide_Rate.csv"
            },
            "key": "Country",
            "fields": ["Age-standardized death rate from self-harm among both sexes"]
          }
        }
      ],
      "mark": {"type": "geoshape", "stroke": "darkgray"},
      "encoding": {
        "color": {
          "field": "Age-standardized death rate from self-harm among both sexes",
          "type": "quantitative",
          "title": "Suicides Per 100,000",
          "scale": {
            "type": "threshold",
            "domain": [5, 10, 15],
            "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
          }
        },
        "tooltip": [
          {"field": "properties.NAME", "type": "nominal", "title": "Country"},
          {"field": "Age-standardized death rate from self-harm among both sexes", "type": "quantitative", "title": "Suicides", "format": ".2f"}
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Bot1234-dotcom/3179_Homework_W9/refs/heads/main/js/WorldMapwithGraticules.topojson",
        "format": {"type": "topojson", "feature": "ne_110m_graticules_30"}
      },
      "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
    }
  ]
}